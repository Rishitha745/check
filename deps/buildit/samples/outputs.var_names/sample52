int isEven (int arg0) {
  if (arg0 == 0) {
    return 1;
  } 
  if (arg0 == 1) {
    return 0;
  } 
  if (arg0 == 2) {
    return 1;
  } 
  if (arg0 == 3) {
    return 0;
  } 
  if (arg0 == 4) {
    return 1;
  } 
  if (arg0 == 5) {
    return 0;
  } 
  if (arg0 == 6) {
    return 1;
  } 
  if (arg0 == 7) {
    return 0;
  } 
  if (arg0 == 8) {
    return 1;
  } 
  if (arg0 == 9) {
    return 0;
  } 
  if (arg0 == 10) {
    return 1;
  } 
  if (arg0 == 11) {
    return 0;
  } 
  if (arg0 == 12) {
    return 1;
  } 
  if (arg0 == 13) {
    return 0;
  } 
  if (arg0 == 14) {
    return 1;
  } 
  return 0;
}

